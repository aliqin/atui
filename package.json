        "babel-loader": "^6.1.0",
        "babel-plugin-transform-runtime": "^6.1.18",
        "babel-preset-es2015": "^6.1.18",
        "babel-runtime": "^6.1.18",
        "css-loader": "^0.21.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "gh-pages": "^0.8.0",
        "jade": "^1.11.0",
        "jade-loader": "~0.8.0",
        "less-loader": "^2.2.0",
        "prismjs": "^1.3.0",
        "style-loader": "^0.13.0",
        "vue-hot-reload-api": "^1.2.0",
        "vue-html-loader": "^1.0.0",
        "vue-loader": "7.1.7",
        "webpack": "^1.12.2",
