  },
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src",
    "lib",
    "dist"
  ],
  "keywords": [
    "atui",
    "vue-component",
    "aliqin"
  ],
  "devDependencies": {
    "antd": "^1.6.3",
    "antd-tools": "^0.10.0",
    "autoprefixer": "~6.3.6",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
