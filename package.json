    "webpack-dev-server": "^1.12.0"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "scripts": {
    "lint": "npm run eslint && npm run lesshint",
    "eslint": "eslint --fix --ext .js,.vue src",
    "dev:test": "karma start build/karma.dev.config.js",
    "lesshint": "lesshint src/styles -r scripts/lesshint-report.js",
    "publish-npm": "npm run authors && tnpm publish",
    "build": "npm run lint && NODE_ENV=production webpack --config build/webconfig.config.js --progress && cp node_modules/vue/dist/vue.js dist/vue.js && cp node_modules/vue/dist/vue.min.js dist/vue.min.js",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "karma start build/karma.unit.config.js",
    "test:cover": "karma start build/karma.cover.config.js",
    "authors": "git log --format='%aN <%aE>' | sort -u  > AUTHORS.txt",
    "start": "webpack-dev-server --config build/webconfig.config.js --host 0.0.0.0 & bisheng start -c ./site/bisheng.config.js",
    "site": "bisheng build -c ./site/bisheng.config.js && sh post-site.sh && sh sync_doc.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "lib": "npm run transform && NODE_ENV=production webpack --config build/webpack.lib.js --progress",
    "transform": "babel src/components/_utils -d lib/_utils && babel src/directives -d lib/directives && babel src/filters -d lib/filters"
  },
  "author": "aliqin fe team",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/aliqin/atui/issues"
  },
  "tnpm": {
    "mode": "npm"
  }
}
